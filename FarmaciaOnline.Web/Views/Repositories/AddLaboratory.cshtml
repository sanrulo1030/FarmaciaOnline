@model FarmaciaOnline.Web.Models.Laboratory
@{
 ViewData["Title"] = "Add Laboratory";
}
<h2>Add</h2>
<h4>Laboratory</h4>
<hr />
<div class="row">
 <div class="col-md-4">
 <form asp-action="AddLaboratory">
 <div asp-validation-summary="ModelOnly" class="text-danger"></div>
 <input type="hidden" asp-for="IdMedicine" />
 <partial name="_Laboratory" />
 <div class="form-group">
 <input type="submit" value="Save" class="btn btn-primary" />
 <a asp-action="DetailsMedicine" asp-route-id="@Model.IdMedicine" class="btn btn-success">Back to List</a>
 </div>
 </form>
 </div>
</div>
@section Scripts {
 @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}